    package="org.weex.plugin.weexscanqr"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="25" />

    <application
        android:icon="@mipmap/ic_launcher"
